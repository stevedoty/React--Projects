{
	"presets": ["es2015", "react", "stage-1"],
	"plugins": [
		["module-resolver", {
			"alias": {
				"ReactApp": "./src",
				"utils": "./src/utils/",
				"Test": "./test"
			}
		}]
	],
	"env": {
		"test": {
			"plugins": [
				"transform-decorators-legacy",
				"transform-object-rest-spread",
				"istanbul"
			]
		}
	}
}
